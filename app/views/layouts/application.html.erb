<!DOCTYPE html>
<html>
<head>
  <title>Twendn</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'http://d3js.org/topojson.v1.min.js' %>
  <%= javascript_include_tag 'https://code.jquery.com/jquery-2.1.1.js' %>
  <%= csrf_meta_tags %>
  <link href='http://fonts.googleapis.com/css?family=Muli:300,400|Oxygen:400,300,700' rel='stylesheet' type='text/css'>
</head>
<body>
<header>
	<% if current_user %>
	  <%= link_to 'Sign Out', signout_path, class: 'signout btn' %>
	<% else %>
	  <%= link_to 'Sign In With Twitter', '/auth/twitter', class: 'signin btn' %>  
	<% end %>
</header>
<%= yield %>

</body>
</html>
